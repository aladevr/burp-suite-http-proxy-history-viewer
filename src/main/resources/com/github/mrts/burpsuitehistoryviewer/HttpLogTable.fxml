<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minWidth="500.0" prefHeight="309.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.102"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.github.mrts.burpsuitehistoryviewer.controller.HttpLogViewController">
    <children>
        <TableView fx:id="httpLogtable" prefHeight="-1.0" prefWidth="-1.0" tableMenuButtonVisible="false"
                   AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                   AnchorPane.topAnchor="40.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
            <columns>
                <TableColumn fx:id="timeColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Time"/>
                <TableColumn fx:id="urlColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="URL"/>
                <TableColumn fx:id="hostColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Host"/>
                <TableColumn fx:id="portColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Port"/>
                <TableColumn fx:id="protocolColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Protocol"/>
                <TableColumn fx:id="methodColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Method"/>
                <TableColumn fx:id="pathColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Path"/>
                <TableColumn fx:id="extensionColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Extension"/>
                <TableColumn fx:id="requestColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Request"/>
                <TableColumn fx:id="statusColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Status"/>
                <TableColumn fx:id="responseLengthColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Response length"/>
                <TableColumn fx:id="mimeTypeColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="MIME type"/>
                <TableColumn fx:id="responseColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Response"/>
                <TableColumn fx:id="commentColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="120.0"
                             text="Comment"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
